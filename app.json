{
	"expo": {
		"name": "Letras",
		"slug": "Letras",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#000"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"bundleIdentifier": "com.f3l1p.letras",
			"supportsTablet": true,
			"buildNumber": "1.0.0"
		},
		"android": {
			"package": "com.f3l1p.letras",
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"permissions": ["RECORD_AUDIO"]
		},

		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
